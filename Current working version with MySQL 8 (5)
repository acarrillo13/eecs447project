CREATE TABLE User (
    user_id INTEGER PRIMARY KEY,
    username VARCHAR(20) NOT NULL UNIQUE,
    role VARCHAR(10),
    CONSTRAINT chk_role CHECK (role IN ('Admin', 'Member')));
   
CREATE TABLE pays_for (
    checkout_id INTEGER PRIMARY KEY,
    user_id INTEGER REFERENCES User,
    amount INTEGER NOT NULL CHECK (amount >= 0));
  
  
-- would we want to change all of the timestamp values to just date?  
-- if we do I would suggest keeping the reserve start and end as a time stamp 
-- so the time of the day could be a tie breaker and keep it the same for download 
-- becuase it doesnt matter 
CREATE TABLE checks_out (
    checkout_id INTEGER PRIMARY KEY,
    user_id INTEGER REFERENCES User,
    media_id varchar(10) REFERENCES Media,
    checkout_date TIMESTAMP NOT NULL,
    due_date TIMESTAMP NOT NULL DEFAULT (CURRENT_TIMESTAMP + INTERVAL 14 DAY),
    return_date TIMESTAMP NULL DEFAULT NULL,
    CONSTRAINT valid_due_date CHECK (due_date >= checkout_date),
    CONSTRAINT valid_return_date CHECK (return_date >= checkout_date));
    
CREATE TABLE downloads (
    download_id INTEGER PRIMARY KEY,
    user_id INTEGER REFERENCES User,
    media_id VARCHAR(10) REFERENCES Media,
    download_date TIMESTAMP NOT NULL);
    
CREATE TABLE reserves (
    reserves_id INTEGER PRIMARY KEY,
    user_id INTEGER REFERENCES User,
    media_id VARCHAR(10) REFERENCES Media,
    reserve_start TIMESTAMP NOT NULL,
    reserve_end TIMESTAMP NULL DEFAULT NULL,
    CONSTRAINT valid_reserve_date CHECK (reserve_end >= reserve_start));
    
/*
I think we should make the media id start with something
that will tell us about what kind of media we are talking about
so that we can then know what media table to join on, like book
can start with BK00000001 or movie can start with MV000001 and so on.
My suggestion is:
Book = BK
Movie = MV
DigitalMedia = DM
Magazine = MG
*/
CREATE TABLE Media (
    media_id VARCHAR(10) PRIMARY KEY,
    date_published DATE);
    
CREATE TABLE Book (
    media_id varchar(10) PRIMARY KEY REFERENCES Media(media_id) ON DELETE CASCADE,
    title VARCHAR(255) NOT NULL,
    author VARCHAR(255),
    genre VARCHAR(100),
    is_reserved BOOLEAN DEFAULT FALSE
);

CREATE TABLE Movie (
    media_id VARCHAR(10) PRIMARY KEY REFERENCES Media(media_id) ON DELETE CASCADE,
    title VARCHAR(255) NOT NULL,
    genre VARCHAR(100),
    director VARCHAR(255),
    publisher VARCHAR(255),
    is_reserved BOOLEAN DEFAULT FALSE
);

CREATE TABLE Magazine (
    media_id varchar(10) PRIMARY KEY REFERENCES Media(media_id) ON DELETE CASCADE,
    volume_no INTEGER,
    issue_no INTEGER,
    title VARCHAR(255) NOT NULL,
    genre VARCHAR(100),
    publication VARCHAR(255),
    is_reserved BOOLEAN DEFAULT FALSE
);

CREATE TABLE DigitalMedia (
    media_id varchar(10) PRIMARY KEY REFERENCES Media(media_id) ON DELETE CASCADE,
    title VARCHAR(255) NOT NULL,
    genre VARCHAR(100),
    publisher VARCHAR(255),
    file_type VARCHAR(50),
    CONSTRAINT chk_file_type CHECK (file_type IN ('epub', 'mp4', 'mp3', 'pdf'))
);

